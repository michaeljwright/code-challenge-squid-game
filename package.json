  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "openapi": "swagger-jsdoc -d openapi-defs.json -o openapi.json 'src/routes/*.ts'",
